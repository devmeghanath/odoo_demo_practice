<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="estate_property_tag_view" model="ir.actions.act_window">
        <field name="name">Propety Tag</field>
        <field name="res_model">real.estate.property.tag</field>
        <field name="view_mode">tree,form</field>
    </record>
    

    <!-- tree view -->
    <record id="estate_property_tag_view_tree" model="ir.ui.view">
        <field name="name">estate_property_tag_tree_view</field>
        <field name="model">real.estate.property.tag</field>
        <field name="arch" type="xml">
            <tree string="estate_property_tag_tree_view">
                <field name="name" />
            </tree>
        </field>
    </record>

    <!-- from view -->

    <record id="estate_property_view_tag_form" model="ir.ui.view">
        <field name="name">estate_property_type_form_view</field>
        <field name="model">real.estate.property.tag</field>
        <field name="arch" type="xml">

            <form string="realestate_tag_form_view">

                <sheet>
                    <group>
                    <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>

    </record>
        <!-- search -->
         
    <record id="estate_property_tag_view_search" model="ir.ui.view">
        <field name="name">estate_property_tag_search_view</field>
        <field name="model">real.estate.property.tag</field>
        <field name="arch" type="xml">
            <search string="tag">
                <field name="name"/>
            </search>
        </field>
    </record>


</odoo>
