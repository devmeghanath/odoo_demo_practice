<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="estate_property_type_view" model="ir.actions.act_window">
        <field name="name">Propety Type</field>
        <field name="res_model">estate.property.type</field>
        <field name="view_mode">tree,form</field>
    </record>


    <!-- tree view -->
     <record id="estate_property_type_view_tree" model="ir.ui.view">
        <field name="name">estate_property_type_tree_view</field>
        <field name="model">estate.property.type</field>
        <field name="arch" type="xml">
            <tree string="estate_property_tree_view">
                <field name="type" />
            </tree>
        </field>
    </record>



    <!-- form view -->



    <record id="estate_property_view_type_form" model="ir.ui.view">
        <field name="name">estate_property_type_form_view</field>
        <field name="model">estate.property.type</field>
        <field name="arch" type="xml">

            <form string="realestate_type_form_view">

                <sheet>
                    <group>
                    <field name="type" />
                    </group>
                </sheet>
            </form>
        </field>


        <!-- search -->
    </record>
    <record id="estate_property_type_view_search" model="ir.ui.view">
        <field name="name">estate_property_type_search_view</field>
        <field name="model">estate.property.type</field>
        <field name="arch" type="xml">
            <search string="Type">
                <field name="type"/>
            </search>
        </field>
    </record>


</odoo>
